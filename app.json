{
	"expo": {
		"name": "weatherly",
		"slug": "weatherly",
		"version": "1.0.0",
		"scheme": "weatherly",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash-img.jpeg",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"experiments": {
			"tsconfigPaths": true
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/cloudy.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/cloudy.png",
			"web": {
				"bundler": "metro"
			}
		}
	}
}
